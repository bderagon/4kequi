      <Y>180</Y>
    </Location>
    <Size>
      <CX>420</CX>
      <CY>270</CY>
    </Size>
    <Text>Change Pet Name</Text>
    <Style_Transparent>false</Style_Transparent>
    <!--<TooltipReference/>-->
