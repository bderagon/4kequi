    <TextColor>
      <R>255</R>
      <G>255</G>
      <B>255</B>
    </TextColor>
    <Template>BDT_Normal</Template>
  </Button>
  <Button item="NCPW_SubmitButton">
    <ScreenID>NCPW_SubmitButton</ScreenID>
    <!--<Font>3</Font>-->
