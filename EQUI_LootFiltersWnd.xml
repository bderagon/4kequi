	<Listbox item="LF_ItemOptionsList">
		<ScreenID>LF_ItemOptionsList</ScreenID>
		<DrawTemplate>WDT_Inner</DrawTemplate>
		<Style_VScroll>true</Style_VScroll>
		<Style_HScroll>false</Style_HScroll>
		<AutoStretch>true</AutoStretch>
		<LeftAnchorOffset>10</LeftAnchorOffset>
		<TopAnchorOffset>80</TopAnchorOffset>
		<RightAnchorOffset>10</RightAnchorOffset>
		<BottomAnchorOffset>10</BottomAnchorOffset>
		<TopAnchorToTop>true</TopAnchorToTop>
		<BottomAnchorToTop>false</BottomAnchorToTop>
		<LeftAnchorToLeft>true</LeftAnchorToLeft>
		<RightAnchorToLeft>false</RightAnchorToLeft>
		<TextColor>
			<R>0</R>
			<G>148</G>
			<B>255</B>
		</TextColor>
		<Location>
			<X>10</X>
			<Y>115</Y>
		</Location>
		<Size>
			<CX>350</CX>
			<CY>280</CY>
		</Size>
		<Style_Border>true</Style_Border>
		<Style_VScroll>true</Style_VScroll>
		<Columns>
			<Width>30</Width>
			<Heading>Item</Heading>
		</Columns>	
		<Columns>
			<Width>10</Width>
			<Heading></Heading>
		</Columns>	
		<Columns>
			<Width>150</Width>
			<Heading>Item Name</Heading>
		</Columns>
		<Columns>
			<Width>25</Width>
			<Heading>AN</Heading>
			<Tooltip>Always Need: Always roll need on this item</Tooltip>
		</Columns>	
		<Columns>
			<Width>25</Width>
			<Heading>AG</Heading>
			<Tooltip>Always Greed: Always roll greed on this item</Tooltip>
		</Columns>	
		<Columns>
			<Width>25</Width>
			<Heading>NV</Heading>
			<Tooltip>Never loot this item</Tooltip>
		</Columns>	
		<Columns>
			<Width>40</Width>
			<Header>Header_AutoRoll</Header>
			<Sortable>true</Sortable>
			<Tooltip>When you are Master Looter, automatically Ask members Need / Greed, and auto Roll after the Ask action.</Tooltip>
		</Columns>
		<Columns>
			<Width>50</Width>
			<Heading>Remove</Heading>
			<Tooltip>Remove this item from filters.</Tooltip>
		</Columns>	
		<TooltipReference>Set loot options here for each loot item.</TooltipReference>
	</Listbox>
